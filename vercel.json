{
  "rewrites": [
    {
      "source": "/api/ai",
      "destination": "https://magic.solutionsuite.cn/api/ai"
    },
    {
      "source": "/functions/v1/:path*",
      "destination": "https://xqsviesaffzksjuqbxey.supabase.co/functions/v1/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/ai",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type,Authorization,Codoc-Nonce,Codoc-Timestamp,Codoc-Signature,Codoc-Env" }
      ]
    }
  ]
}
